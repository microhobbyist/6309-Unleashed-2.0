 54:
 55:/* $FE00-$FEFF (256B) : Register range, or future expansion */
 56:BlockREG =  Fxxx & (A11 & A10 & A9 & !A8) ;
 57:
 58:/* $FC00-$FDFF (512B) : MMU Task/Page registers */
 59:BlockPAGE =  Fxxx & A11 & A10 & !A9 ;
 60:
 61:/* $0000-$FBFF + $FFF0-$FFFF (63KB): General memory area) */
 62:BlockMEM = (!(Fxxx & A11 & A10)) # FFFx ;
 63:
 64:/* Output Equations (active-HIGH internal) */
 65:CONST   = BlockCONST & VMA ;
 66:IORQ    = BlockIORQ  & VMA ;
 67:REG     = BlockREG   & VMA ;
 68:PAGE    = BlockPAGE  & VMA ;
 69:MREQ    = BlockMEM   & VMA ;
